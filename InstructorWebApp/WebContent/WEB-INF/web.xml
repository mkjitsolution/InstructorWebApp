<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>InstructorWebApp</display-name>
  <welcome-file-list>
    <welcome-file>InstructorInputPage.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>InstructorServlet</display-name>
    <servlet-name>InstructorServlet</servlet-name>
    <servlet-class>com.mkj.insapp.web.InstructorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InstructorServlet</servlet-name>
    <url-pattern>/InstructorServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ViewAllInstructor</display-name>
    <servlet-name>ViewAllInstructor</servlet-name>
    <servlet-class>com.mkj.insapp.web.ViewAllInstructor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewAllInstructor</servlet-name>
    <url-pattern>/ViewAllInstructor</url-pattern>
  </servlet-mapping>
</web-app>